<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

        .caption {
            font-size: 12pt;
            font-weight: bold;
            margin-bottom: 8px;
        }

        .light {
            color: #888;
        }

        .myselfSpacer {
            margin: -10px 0 0 18px;
            padding-bottom: 10px;
            font-size: 14pt;
            font-weight: bold;
            color: #888;
        }

    </ui:style>
    <g:HTMLPanel>
        <div class="{style.caption}">Top 5:</div>
        <g:HTMLPanel ui:field="persons"/>
        <g:HTMLPanel ui:field="myselfSpacer" styleName="{style.myselfSpacer}" visible="false">
            ...
        </g:HTMLPanel>
        <g:HTMLPanel ui:field="myself"/>
        <g:Label ui:field="empty" styleName="{style.light}" text="Get busy and become the first on this list." visible="false"/>
    </g:HTMLPanel>
</ui:UiBinder> 