<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
        @eval tpadding com.c5000.mastery.client.framework.PageContainer.PADDING_TOP_CSS;
        @eval vpadding com.c5000.mastery.client.framework.PageContainer.PADDING_CSS;
        @eval width com.c5000.mastery.client.framework.PageContainer.WIDTH_CSS;
        @eval borderWidth com.c5000.mastery.client.framework.PageContainer.BORDER_WIDTH_CSS;
        .outer {
            width: width;
            padding: tpadding vpadding vpadding;
            background-color: #FFFFFF;
            border-right: borderWidth solid #DDD;
        }
	</ui:style>
    <g:HTMLPanel ui:field="pageContainer" styleName="floatLeft {style.outer}"/>
</ui:UiBinder> 